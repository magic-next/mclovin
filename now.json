{
  "version": 2,
  "alias": "magic-mclovin",
  "builds": [
    {
      "src": "/src/app.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/src/app.js" }
  ],
  "build": {
    "env": {
      "BASE_URL": "https://magic-mclovin.now.sh",
      "FIREBASE_ADMIN": "@firebase-admin",
      "SALT": "@salt",
      "PRIVATE_KEY": "@access-key",
      "ICQ_URL": "https://magic-icq.now.sh",
      "ICQ_KEY": "@icq-token"
    }
  },
  "env": {
    "BASE_URL": "https://magic-mclovin.now.sh",
    "FIREBASE_ADMIN": "@firebase-admin",
    "SALT": "@salt",
    "PRIVATE_KEY": "@access-key",
    "ICQ_URL": "https://magic-icq.now.sh",
    "ICQ_KEY": "@icq-token"
  }
}